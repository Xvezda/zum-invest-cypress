{
    "baseUrl": "https://invest.zum.com",
    "chromeWebSecurity": false,
    "defaultCommandTimeout": 10000,
    "viewportWidth": 1280,
    "viewportHeight": 800,
    "retries": {
        "runMode": 9,
        "openMode": 0
    },
    "env": {
        "cypress-plugin-snapshots": {
            "autoCleanUp": false,
            "autopassNewSnapshots": true,
            "diffLines": 3,
            "excludeFields": [], 
            "ignoreExtraArrayItems": false, 
            "ignoreExtraFields": false, 
            "normalizeJson": true, 
            "prettier": true, 
            "imageConfig": {
                "createDiffImage": true, 
                "resizeDevicePixelRatio": true, 
                "threshold": 0.05, 
                "thresholdType": "percent" 
            },
            "screenshotConfig": { 
                "blackout": [".stock_state_chart", ".ticker_bar", ".il_noti"],
                "capture": "fullPage",
                "clip": null,
                "disableTimersAndAnimations": true,
                "log": false,
                "scale": false,
                "timeout": 30000
            },
            "serverEnabled": true, 
            "serverHost": "localhost", 
            "serverPort": 2121, 
            "updateSnapshots": false, 
            "backgroundBlend": "difference" 
        }
    }
}