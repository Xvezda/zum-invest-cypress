{
    "baseUrl": "https://invest.zum.com",
    "video": false,
    "chromeWebSecurity": false,
    "numTestsKeptInMemory": 0,
    "defaultCommandTimeout": 10000,
    "requestTimeout": 10000,
    "viewportWidth": 1280,
    "viewportHeight": 800,
    "retries": {
        "runMode": 2,
        "openMode": 0
    },
    "blockHosts": [
        "*.g.doubleclick.net",
        "*.googlesyndication.com",
        "*.googleadservices.com",
        "*.googletagmanager.com",
        "analytics.google.com",
        "*.criteo.com",
        "*.vrixon.com",
        "*.onkakao.net",
        "*.dable.io",
        "wcs.naver.net",
        "estat.zum.com",
        "displayad.zum.com"
    ],
    "env": {
        "cypress-plugin-snapshots": {
            "autoCleanUp": false,
            "autopassNewSnapshots": true,
            "diffLines": 3,
            "excludeFields": [], 
            "ignoreExtraArrayItems": false, 
            "ignoreExtraFields": false, 
            "normalizeJson": true, 
            "prettier": true, 
            "imageConfig": {
                "createDiffImage": true, 
                "resizeDevicePixelRatio": true, 
                "threshold": 0.03,
                "thresholdType": "percent" 
            },
            "screenshotConfig": { 
                "blackout": [".stock_state_chart", ".ticker_bar", ".il_noti", ".main_indicator .chart"],
                "capture": "fullPage",
                "clip": null,
                "disableTimersAndAnimations": true,
                "log": false,
                "scale": false,
                "timeout": 30000
            },
            "serverEnabled": true, 
            "serverHost": "localhost", 
            "serverPort": 2121, 
            "updateSnapshots": false, 
            "backgroundBlend": "difference" 
        }
    }
}
